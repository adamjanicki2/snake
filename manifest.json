{
  "name": "Snake",
  "description": "The classic JavaScript Snake recreated for easy use!",
  "version": "1.3.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "./index.html",
    "default_icon": {
      "16": "./16.png",
      "32": "./32.png",
      "48": "./48.png",
      "128": "./128.png"
    }
  },
  "icons": {
    "16": "./16.png",
    "32": "./32.png",
    "48": "./48.png",
    "128": "./128.png"
  }
}
